{
    "manifest_version":2,
    "name":"open links to tabs",
    "description":"open all tickets in the current page into separate tabs",
    "version": "1.0.0",
    "icons":{"128":"icon_128.png"},
    "browser_action":{
        "default_icon":"icon.png"
        
    },
    "background": {
        "persistent": false,
        "scripts": ["background_script.js"]
      },
    "permissions":["activeTab"],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ]

}